<div id="<%= dom_id revenue %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= revenue.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= revenue.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Amount:</strong>
    <%= revenue.amount %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Date:</strong>
    <%= revenue.date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= revenue.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= revenue.category_id %>
  </div>
</div>
